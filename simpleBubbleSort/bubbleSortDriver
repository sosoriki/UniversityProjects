package bubbleSort;

public class driver {
	public static void main(String[] arg) {
		int[] array = {21, 4, -9, 15, 97, 26, 42, -17};
		for(int i = 0; i < array.length; i++) {
			for(int j = 0; j < i; j++) {
				if(array[j] > array[i]) {
					int temp = array[j];
					array[j] = array[i];
					array[i] = temp;
				}
			}
		}
		for(int i = 0; i < array.length; i++) {
			System.out.printf("%d,", array[i]);
		}
	}

}
